#!ipxe

# Antergos Operating System
# https://antergos.com

goto ${menu}

:antergos
set os Antergos
set antergos_version latest
menu ${os} Installers
item x86_64 ${space} ${os} ${antergos_version} Minimal 64bit [ISO]
choose antergos_arch || goto antergos_exit
goto antergos_boot

:antergos_boot
kernel ${memdisk} iso raw
initrd https://antergos.com/download/antergos-live-iso/
boot
goto antergos_exit

:antergos_exit
clear menu
exit 0
